{% import "macros/macros.njk" as macro %}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{{title}}</title>{{macro.css(s="style")}}</head>
<body>
    <nav class="top-nav">
        <div class="nav-wrapper {{config.theme.main}}"><a href="#" class="brand-logo">Admin</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
              {% for item in config.entries %}
                <li><a href="{{item}}">{{item|capitalize}}</a></li>
              {% endfor %}
            </ul>
        </div>
    </nav>
    {% block content %}{% endblock %}
	<div id="scroll-to-top" class="{{config.theme.main}}"><i class="icon-angle-up"></div>
    {{macro.js(s="way")}}
	{{macro.js(s="app")}}
    {%block scripts%}{% endblock %}
  </body>
</html>
